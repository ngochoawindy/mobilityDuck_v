require mobilityduck

query I
SELECT intspan('[1,2]');
----
[1, 3)

query I
SELECT intspan ('(1,2]');
----
[2, 3)

query I
SELECT tstzspan('[2000-01-01,2000-01-01]');
----
[2000-01-01 00:00:00+00, 2000-01-01 00:00:00+00]

query I
SELECT tstzspan('[2000-01-01,2000-01-03]') @> timestamptz '2000-01-02';
----
true

query I
SELECT tstzspan('[2000-01-01,2000-01-03]') @> timestamptz '2000-01-10';
----
false

query I
SELECT intspan '[1, 3]' * intspan '[3, 5]';
----
[3, 4)

query I
SELECT bigintspan '[1000, 3000]' * bigintspan '[3000, 5000]';
----
[3000, 3001)

query I
SELECT floatspan '[1, 3]' * floatspan '[1, 3]';
----
[1, 3]

query I
SELECT floatspan '[1, 3]' * floatspan '(3, 5]';
----
NULL

query I
SELECT datespan '[2000-01-01,2000-01-02]' * datespan '[2000-01-01,2000-01-02]';
----
[2000-01-01, 2000-01-03)

query I
SELECT tstzspan '[2000-01-01, 2000-01-05]' * tstzspan '[2000-01-03, 2000-01-08]';
----
[2000-01-03 00:00:00+00, 2000-01-05 00:00:00+00]