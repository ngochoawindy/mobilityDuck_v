query I
SELECT tempDump(tfloat '[1.5@2001-01-01, 2.5@2001-01-02, 1.5@2001-01-03]');
----
[{'value': 1.5, 'time': '{[2001-01-01 00:00:00+00, 2001-01-01 00:00:00+00], [2001-01-03 00:00:00+00, 2001-01-03 00:00:00+00]}'}, {'value': 2.5, 'time': '{[2001-01-02 00:00:00+00, 2001-01-02 00:00:00+00]}'}]

query I
SELECT atValues(tfloat 'Interp=Step;{[1.5@2000-01-01, 2.5@2000-01-02, 1.5@2000-01-03],[3.5@2000-01-04, 3.5@2000-01-05]}', floatspan '[1,3]');
----
Interp=Step;{[1.5@2000-01-01 00:00:00+00, 2.5@2000-01-02 00:00:00+00, 1.5@2000-01-03 00:00:00+00]}