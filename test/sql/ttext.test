require mobilityduck

query I
SELECT ttext('AAA', timestamptz '2001-01-01 08:00:00');
----
"AAA"@2001-01-01 08:00:00+00

query I
SELECT ttextSeq(ARRAY[
    ttext('A', timestamptz '2012-01-01 08:00:00'),
    ttext('B', timestamptz '2012-01-01 08:10:00'),
    ttext('C', timestamptz '2012-01-01 08:20:00')
], 'discrete');
----
{"A"@2012-01-01 08:00:00+00, "B"@2012-01-01 08:10:00+00, "C"@2012-01-01 08:20:00+00}

query I
SELECT ttextSeq(ARRAY[
    'A@2012-01-01 08:00:00'::ttext,
    'B@2012-01-01 08:10:00'::ttext,
    'C@2012-01-01 08:20:00'::ttext
], 'discrete');
----
{"A"@2012-01-01 08:00:00+00, "B"@2012-01-01 08:10:00+00, "C"@2012-01-01 08:20:00+00}

query I
SELECT ttextSeq(ARRAY['A@2025-01-01 08:00:00'::TTEXT]) as ttext_seq;
----
["A"@2025-01-01 08:00:00+00]

query I
SELECT ttextSeq(ARRAY[
    ttext('A', timestamptz '2025-01-01 08:00:00')
]) as ttext_seq;
----
["A"@2025-01-01 08:00:00+00]

query I
SELECT tempDump(ttext '["AAA"@2001-01-01, "BBB"@2001-01-02, "AAA"@2001-01-03]');
----
[{'value': AAA, 'time': '{[2001-01-01 00:00:00+00, 2001-01-02 00:00:00+00), [2001-01-03 00:00:00+00, 2001-01-03 00:00:00+00]}'}, {'value': BBB, 'time': '{[2001-01-02 00:00:00+00, 2001-01-03 00:00:00+00)}'}]

query I
SELECT atMin(ttext '{[AAA@2000-01-01, BBB@2000-01-02, AAA@2000-01-03],[CCC@2000-01-04, CCC@2000-01-05]}');
----
{["AAA"@2000-01-01 00:00:00+00, "AAA"@2000-01-02 00:00:00+00), ["AAA"@2000-01-03 00:00:00+00]}

query I
SELECT instants(ttext '{[AAA@2000-01-01, BBB@2000-01-02, AAA@2000-01-03],[CCC@2000-01-04, CCC@2000-01-05]}');
----
['"AAA"@2000-01-01 00:00:00+00', '"BBB"@2000-01-02 00:00:00+00', '"AAA"@2000-01-03 00:00:00+00', '"CCC"@2000-01-04 00:00:00+00', '"CCC"@2000-01-05 00:00:00+00']